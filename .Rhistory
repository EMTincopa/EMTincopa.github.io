#libraries
library(readxl)
library(echarts4r)
library(tidyverse)
library(dplyr)
library(highcharter)
#load data
hiring_data<-read_excel("D:/Dataviz & Information Design - Projects/Makeovermonday/Week 34/Data/Entry-Level LinkedIn Jobs.xlsx", sheet = 1)
hiring_data %>%
mutate(Experience_percent = Experience * 100) %>%
hchart(type = "bar",
hcaes(x = Job, y = Experience_percent),
color = c("#0D3C38"),
name = "Entry-level job") %>%
hc_title(
text = "<b>Are Entry-Level jobs really Entry-Level?</b>",
margin = 20,
align = "left",
style = list(
color = "#333333",
useHTML = TRUE,
fontSize = "21px",
fontFamily = "Roboto condensed"
)
) %>%
hc_subtitle(
text = "Original article: Entry-level jobs on LinkedIn require 3+ years of experience",
margin = 20,
align = "left",
style = list(
fontFamily = "Roboto",
fontSize = "14px",
color = "#666666"
)
) %>%
hc_caption(text = "Design: Enrique Mendoza | Source: LinkedIn, #MakeOverMonday week 34, 2021",
style = list(fontFamily = "Roboto", fontSize = "10px")) %>%
hc_chart(backgroundColor = "#F6F3EC") %>%
hc_tooltip(
crosshairs = TRUE,
backgroundColor = "#FFC876",
shared = TRUE,
borderWidth = 1,
valueSuffix = " %",
style = list(fontFamily = "Roboto condensed", fontSize = "14")
) %>%
hc_yAxis(title = list(text = ""),
labels = list(
format = "{value}%",
style = list(
fontFamily = "Roboto",
fontSize = "12px",
color = "#666666"
)
)) %>%
hc_xAxis(title = list(text = ""), labels = list(
style = list(
fontFamily = "Roboto",
fontSize = "12px",
color = "#333333"
)
)) %>%
hc_exporting(enabled = TRUE)
